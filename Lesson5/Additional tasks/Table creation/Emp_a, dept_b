EMP_A

CREATE TABLE `sql`.`emp_a` (
`EMPID` INT NOT NULL,
`EMPNAME` VARCHAR(50) NULL,
`MGRID` VARCHAR(45) NULL,
`DEPTID` VARCHAR(45) NULL,
`SALARY` FLOAT NULL DEFAULT NULL,
`DOB` DATETIME NULL,
PRIMARY KEY (`EMPID`),
UNIQUE INDEX `EMPID_UNIQUE` (`EMPID` ASC) VISIBLE);


DEPT_B

CREATE TABLE `sql`.`dept_b` (
`DEPTID` INT NOT NULL,
`DEPTNAME` VARCHAR(45) NULL,
PRIMARY KEY (`DEPTID`));
